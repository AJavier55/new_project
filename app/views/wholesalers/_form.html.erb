<% if flash[:my_errors]%>
<%flash[:my_errors].each do |error|%>
<p><%= error %></p>
<%end%>
<%end%>

<%= form_for @wholesaler do |f|%>
<%= f.label :name %>
<%= f.text_field :name%>
<%= f.label :location %>
<%= f.text_field :location%>
<%= f.label :business_type %>
<%= f.text_field :occupation%>
<% @wholesaler.wholesaler_products.each do |wholesaler_product|%>
<%= wholesaler_product.product.name%>
<%= f.label :product%>
<%= f.text_field :product %>
<%end%>
<%= f.submit%>
<%end%>